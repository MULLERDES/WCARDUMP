@model CarDumpApp.Models.SearchSetsViewModel


<div class="container" id="SearchResultSets" style="margin-top:20px;">

    <table class="table">
        <tr style="background-color:#1a6358;color:#ffffff;">
            <th  >
                Id
            </th>
            <th>
                Brand
            </th>
            <th>
                Model
            </th>
            <th>
                Year
            </th>
            
            <th></th>
        </tr>

        @foreach(var item in Model.Sets)
        {
            <tr>
                <td >
                 @Html.ActionLink(item.PersonalNumber,"SetDetails", new {id=item.Id})
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AutoModel.AutoBrand.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AutoModel.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Year)
                </td>
                 
                
                <td style="text-align:right;">
                    @Html.ActionLink("Details", "SetDetails", new { id = item.Id }) 
                </td>
            </tr>
        }

    </table>
</div>